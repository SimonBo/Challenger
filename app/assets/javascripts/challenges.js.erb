var NotificationPoller;

NotificationPoller = {
  poll: function() {
    return setTimeout(this.request, 5000);
  },
  request: function() {
    $('#notifications').html("<%= j render: 'users/user_notifications' %>");
    console.log('Request fired');
    return NotificationPoller.poll();
  }
};

jQuery(function() {
  if ($('#notifications').length > 0) {
    return NotificationPoller.poll();
  }
});
