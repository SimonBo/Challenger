- if user_signed_in?
  = current_user.username
  = link_to 'Edit account', edit_user_registration_path
  = link_to 'Sign out', destroy_user_session_path, :method=>'delete'
  %h3 Accepted Challenges:
  - current_user.dares.each do |i|
    = link_to i.challenge.name, i.challenge
  %h3 People you have challenged:
- else
  = link_to new_user_session_path do
    %button.btn.btn-sm.btn-default Sign in
  = link_to new_user_registration_path do
    %button.btn.btn-sm.btn-default Sign up