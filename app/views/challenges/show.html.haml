%p#notice= notice

%p
  %b Accepted by:
  - @challenge.dares.each do |i|
    - User.where(id: i.acceptor_id).each do|user|
      = user.username
%p
  %b Name:
  = @challenge.name
%p
  %b Description:
  = @challenge.description

= link_to 'Edit', edit_challenge_path(@challenge)
\|
= link_to 'Back', challenges_path
